{
    "manifest_version": 3,
    "name": "Console Logger",
    "version": "1.0",
    "permissions": [
      "activeTab",
      "scripting"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ],
    "host_permissions": [
      "<all_urls>"
    ]
  }
  